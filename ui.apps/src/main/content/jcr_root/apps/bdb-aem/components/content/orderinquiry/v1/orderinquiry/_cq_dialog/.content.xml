<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Orders Inquiry"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <orderInquiryLabels
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Navigation Labels"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <contactUsLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Contact Us Label"
                                                    name="./contactUsLabel"/>
                                            <areaOfInquiryLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Area Of Inquiry Label"
                                                    name="./areaOfInquiryLabel"/>
                                            <mandatoryErrorLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Mandatory Error Label"
                                                    name="./mandatoryErrorLabel"/>
                                            <phoneNumPatternError
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Phone Num Pattern Error"
                                                    name="./phoneNumPatternError"/>
                                            <commentsLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Comments Label"
                                                    name="./commentsLabel"/>
                                            <inquiryNote
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Inquiry Note"
                                                    name="./inquiryNote"/>
                                            <submitBtnLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Submit Btn Label"
                                                    name="./submitBtnLabel"/>
                                            <cancelBtnLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Cancel Btn Label"
                                                    name="./cancelBtnLabel"/>
                                            <tncNote
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                    fieldLabel="Terms and Conditions Note"
                                                    name="./tncNote"
                                                    useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceSuperType="/apps/bdb-aem/components/authoring/dialog/richtext/rtePlugins"/>
                                                <uiSettings
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceSuperType="/apps/bdb-aem/components/authoring/dialog/richtext/uiSettings"/>
                                            </tncNote>
                                            <imagePath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="Image URL"
                                                    fieldLabel="Image URL"
                                                    name="./imagePath"
                                                    required="{Boolean}true"
                                                    rootPath="/content/dam/bdb"/>
                                            <imageAltText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Image Alt Text"
                                                    name="./imageAltText"/>
                                            <successTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Success Title"
                                                    name="./successTitle"/>
                                            <successSubTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Success Sub Title"
                                                    name="./successSubTitle"/>
                                            <askQuestionCTALabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Ask Question CTA Label"
                                                    name="./askQuestionCTALabel"/>
                                            <askQuestionCTALink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="Image URL"
                                                    fieldLabel="Ask Question CTA Link"
                                                    name="./askQuestionCTALink"
                                                    required="{Boolean}true"
                                                    rootPath="/content/bdb"/>
                                            <backToOrderCTALabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Back To Order CTA Label"
                                                    name="./backToOrderCTALabel"/>
                                            <invoiceNumberLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Invoice Number Label"
                                                    name="./invoiceNumberLabel"/>
                                            <emailFormatError
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Email Format Error Message"
                                                    name="./emailFormatError"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </orderInquiryLabels>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>