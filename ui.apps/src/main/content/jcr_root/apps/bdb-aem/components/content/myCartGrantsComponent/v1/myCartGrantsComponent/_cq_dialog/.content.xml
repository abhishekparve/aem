<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Cart Grants"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-button__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <container
                jcr:primaryType="nt:unstructured"
                jcr:title="Properties"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <noGrantsMsg
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="No Grants Message"
                                        maxlength="{Long}150"
                                        name="./noGrantsMsg"/>
                                    <useMyGrantsLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Use My BD Grant text"
                                        name="./useMyGrantsLabel"/>
                                    <grantsLeftLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Grants Left text"
                                        name="./grantsLeftLabel"/>
                                    <changeLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Change CTA label"
                                        name="./change"/>
                                    <grantsInfoTooltipText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Grants Information text"
                                        name="./grantsInfoTooltipText"/>
                                    <selectGrant
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Select Grant title"
                                        name="./selectGrant"/>
                                    <grantsLeft
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Grants Left label"
                                        name="./grantsLeft"/>
                                    <shipToNumber
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Ship to Number label"
                                        name="./shipToNumber"/>
                                    <startingFrom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Starting From label"
                                        name="./startingFrom"/>
                                    <validUntil
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Valid Until label"
                                        name="./validUntil"/>
                                    <userToSelectGrantText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Grants Selection text"
                                        maxlength="{Long}150"
                                        name="./userToSelectGrantText"/>
                                    <cancelBtnText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Cancel CTA label"
                                        name="./cancelBtnText"/>
                                    <useGrantBtnText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Use Grant CTA label"
                                        name="./useGrantBtnText"/>
                                    <grantWarningAltText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Grant Warning Alt text"
                                        name="./grantWarningAltText"/>
                                    <toolTipAltText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Tool Tip Alt text"
                                        name="./toolTipAltText"/>
									<outOfText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Out of text"
                                        name="./outOfText"/>
                                </items>
                            </column>
                        </items>
                    </columns>
                </items>
            </container>
        </items>
    </content>
</jcr:root>
