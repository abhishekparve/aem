<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Quick Add"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Quick Add labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Quick Add Title"
                                        fieldLabel="Quick Add Title"
                                        name="./heading"/>
                                    <addItemsInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Quick Add Text"
                                        fieldLabel="Quick Add Text"
                                        name="./addItemsInfo"/>
                                    <cancel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Cancel text"
                                        fieldLabel="Cancel text"
                                        name="./cancel"/>
                                </items>
                            </columns>
                        </items>
                    </basic>
                    <textEntry
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Text Entry Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <tabTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Tab Label"
                                        fieldLabel="Tab Label"
                                        name="./tabTitle"/>
                                    <textEntryInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Instruction text for user"
                                        fieldLabel="Instruction text for user"
                                        name="./textEntryInfo"/>
									<catalogNumTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Catalog Number label"
                                        fieldLabel="Catalog Number label"
                                        name="./catalogNumTitle"/>
                                    <catalogNum
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Placeholder text for Catalog field"
                                        fieldLabel="Placeholder text for Catalog field"
                                        name="./catalogNum"/>
									<quantityTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Quantity label"
                                        fieldLabel="Quantity label"
                                        name="./quantityTitle"/>
                                    <quantity
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Placeholder text for Quantity field"
                                        fieldLabel="Placeholder text for Quantity field"
                                        name="./quantity"/>
                                    <addMoreItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Label for Add More Items CTA"
                                        fieldLabel="Label for Add More Items CTA"
                                        name="./addMoreItems"/>
                                </items>
                            </columns>
                        </items>
                    </textEntry>
                    <bulkPaste
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Bulk Paste Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <tabTitleBulk
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Tab Label"
                                        fieldLabel="Tab Label"
                                        name="./tabTitleBulk"/>
                                    <bulkPasteInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Instruction text for user"
                                        fieldLabel="Instruction text for user"
                                        name="./bulkPasteInfo"/>
                                    <placeholder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Example text"
                                        fieldLabel="Example text"
                                        name="./placeholder"/>
                                </items>
                            </columns>
                        </items>
                    </bulkPaste>
                    <excelUpload
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Excel Upload Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <tabTitleExcel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Tab Label"
                                        fieldLabel="Tab Label"
                                        name="./tabTitleExcel"/>
                                    <uploadInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Instruction text for user"
                                        fieldLabel="Instruction text for user"
                                        name="./uploadInfo"/>
                                    <downloadInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Text for downloading template"
                                        fieldLabel="Text for downloading template"
                                        name="./downloadInfo"/>
                                    <downloadCTA
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Download template CTA label"
                                        fieldLabel="Download template CTA label"
                                        name="./downloadCTA"/>
									<downloadUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Download template CTA URL"
                                        fieldLabel="Download template CTA URL"
                                        name="./downloadUrl"
                                        rootPath="/content/dam/bdb"/>
									<title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Text for uploading file"
                                        fieldLabel="Text for uploading file"
                                        name="./title"/>
									<note
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Info Text for uploading file"
                                        fieldLabel="Info Text for uploading file"
                                        name="./note"/>
                                    <iconSrc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Upload Icon "
                                        fieldLabel="Upload Icon "
                                        name="./iconSrc"
                                        rootPath="/content/dam/bdb"/>
                                    <iconSrcAltText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Upload Icon Alt Text"
                                        fieldLabel="Upload Icon Alt Text"
                                        name="./altText"/>
                                    <btnName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Upload file CTA label"
                                        fieldLabel="Upload file CTA label"
                                        name="./btnName"/>
                                </items>
                            </columns>
                        </items>
                    </excelUpload>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
