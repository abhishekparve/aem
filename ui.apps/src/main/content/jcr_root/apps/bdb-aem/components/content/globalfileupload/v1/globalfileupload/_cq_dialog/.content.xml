<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Global File Upload Labels"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <fileUpload
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Password"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <fileTypeFieldSet
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="File Type Field Titles"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <fileTypeMultiField
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="File Type"
                                                    fieldLabel="Add Field Titles for file types">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./fileTypeFieldTitleMultiField">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="ID"
                                                                required="{Boolean}true"
                                                                name="./id"/>
                                                        <title
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Field Title"
                                                                required="{Boolean}true"
                                                                name="./label"/>
                                                    </items>
                                                </field>
                                            </fileTypeMultiField>
                                            <fieldTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="File Upload Title Text"
                                                    name="./fieldTitle"
                                                    required="{Boolean}true"/>
                                            <info
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="File Upload Info Text"
                                                    name="./info"
                                                    required="{Boolean}true"/>
                                            <iconSrc
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldLabel="Icon path"
                                                    rootPath="/content/dam/bdb"
                                                    name="./iconSrc"
                                                    required="{Boolean}true"/>
                                            <altText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Icon Alt text"
                                                    name="./altText"
                                                    required="{Boolean}true"/>
                                            <title
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Title"
                                                    name="./title"
                                                    required="{Boolean}true"/>
                                            <note
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Note"
                                                    name="./note"
                                                    required="{Boolean}true"/>
                                            <btnName
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Button Name"
                                                    name="./btnName"
                                                    required="{Boolean}true"/>
                                            <ariaLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Aria Label"
                                                    name="./ariaLabel"
                                                    required="{Boolean}true"/>
                                            <fileIconSrc
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldLabel="File Icon Path"
                                                    rootPath="/content/dam/bdb"
                                                    name="./fileIconSrc"
                                                    required="{Boolean}true"/>
                                            <fileIconAltText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="File Icon Alt Text"
                                                    name="./fileIconAltText"
                                                    required="{Boolean}true"/>
                                            <closeIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                    fieldLabel="Close Icon Path"
                                                    rootPath="/content/dam/bdb"
                                                    name="./closeIcon"
                                                    required="{Boolean}true"/>
                                            <ariaLabelClose
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Aria Label Close"
                                                    name="./ariaLabelClose"
                                                    required="{Boolean}true"/>
                                            <error
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Error Message"
                                                    name="./error"
                                                    required="{Boolean}true"/>

                                        </items>
                                    </fileTypeFieldSet>
                                </items>
                            </column>
                        </items>
                    </fileUpload>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
