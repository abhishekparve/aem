<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Beadlots Master Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <properties
                jcr:primaryType="nt:unstructured"
                jcr:title="Properties"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Bead Lots Title"
                                        name="./title"/>
                                    <searchPlaceholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Bead Lots Search Placeholder"
                                            name="./searchPlaceholder"/>
                                    <searchAllInfo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Search All Information"
                                        name="./searchAllInfo"/>
                                    <searchAllInfoDynamic
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Search All Information Dynamic"
                                            name="./searchAllInfoDynamic"/>
                                    <categoryAll
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Category All"
                                            name="./categoryAll"/>
                                    <toolTipText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Tool Tip Text"
                                            name="./toolTipText"/>
                                    <toolTipImg
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldLabel="Tool Tip Img"
                                            name="./toolTipImg"
                                            rootPath="/content/dam/bdb"/>
                                    <resultsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Results Text"
                                            name="./resultsText"/>
                                    <noResults
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="No Results"
                                            name="./noResults"/>
                                    <suggestedResults
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Suggested Results"
                                            name="./suggestedResults"/>
                                    <noResultsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="No Results Text"
                                            name="./noResultsText"/>
                                    <noResultsCta
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="No Results CTA Label"
                                            name="./noResultsCta"/>
                                    <noSearchResultsIcon
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldLabel="No Search Results Icon"
                                            name="./noSearchResultsIcon"
                                            rootPath="/content/dam/bdb"/>
                                    <noResultsIcon
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                            fieldLabel="No Results Icon"
                                            name="./noResultsIcon"
                                            rootPath="/content/dam/bdb"/>
                                    <altText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Alt Text"
                                            name="./altText"/>
                                    <noSearchResults
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="No Search Results"
                                            name="./noSearchResults"/>
                                    <showStatus
                                    		jcr:primaryType="nt:unstructured"
                                    		sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    		name="./showStatus"
                                    		text="Show Status Flag"
                                    		value="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </columns>
                </items>
            </properties>
        </items>
    </content>
</jcr:root>
