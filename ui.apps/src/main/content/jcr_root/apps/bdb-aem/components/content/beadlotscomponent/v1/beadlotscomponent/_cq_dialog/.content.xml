<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="BeadLots Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <properties
                jcr:primaryType="nt:unstructured"
                jcr:title="Properties"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <categoryLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Bead Lots Category Label"
                                            name="./categoryLabel"/>
                                    <beadlotsCategoryTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Bead Lots Category Title"
                                            name="./beadlotsCategoryTitle"/>
                                    <beadlotsCategorySubTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Bead Lots Category Sub Title"
                                            name="./beadlotsCategorySubTitle"/>
                                    <statusLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Status Label"
                                            name="./statusLabel"/>
                                    <noResult
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="No Result"
                                            name="./noResult"/>
                                    <partNumberLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Part or Material Number Label"
                                            name="./partNumberLabel"/>
                                    <beadlotsFilesTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Beadlots Files Title"
                                            name="./beadlotsFilesTitle"/>
									<showStatus
                                    		jcr:primaryType="nt:unstructured"
                                    		sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    		name="./showStatus"
                                    		text="Show Status Flag"
                                    		value="{Boolean}true"/>
                                    <installationsInstructionsDetailsField
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Installations Instructions"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <installationsInstructions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Add Installation Instructions documents">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./multipleInstallationsInstructions">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <categoryInstallationInstructionsLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Installation Instructions Label"
                                                                name="./categoryInstallationInstructionsLabel"/>
                                                        <categoryInstallationInstructionsLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Installation Instructions Link"
                                                                name="./categoryInstallationInstructionsLink"
                                                                rootPath="/content"/>
                                                    </items>
                                                </field>
                                            </installationsInstructions>
                                        </items>
                                    </installationsInstructionsDetailsField>
                                    <partNumberField
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Part Numbers"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <partNumberMultiField
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Add Part Numbers and Related Details">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./partNumberMultiField">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <partNumbersDetailsField
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <multiplePartNumbers
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        composite="{Boolean}true"
                                                                        fieldLabel="Add Part Numbers and Tags">
                                                                    <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./multiplePartNumbers">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <partNumber
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="Part Number"
                                                                                    required="{Boolean}true"
                                                                                    name="./partNumber"/>
                                                                            <badge
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="Badge for Part Number (Optional)"
                                                                                    name="./badge"/>
                                                                        </items>
                                                                    </field>
                                                                </multiplePartNumbers>
                                                            </items>
                                                        </partNumbersDetailsField>
                                                        <information
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Add more information"
                                                                name="./information"/>
                                                        <installationInstructionsLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Installation Instructions Label"
                                                                name="./installationInstructionsLabel"/>
                                                        <installationInstructionsLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Installation Instructions Link"
                                                                name="./installationInstructionsLink"
                                                                rootPath="/content"/>
                                                        <updaterFileLabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Updater File Label"
                                                                name="./updaterFileLabel"/>
                                                        <updaterFileLink
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldLabel="Updater File Link"
                                                                name="./updaterFileLink"
                                                                rootPath="/content/dam/bdb"/>
                                                    </items>
                                                </field>
                                            </partNumberMultiField>
                                        </items>
                                    </partNumberField>
                                </items>
                            </column>
                        </items>
                    </columns>
                </items>
            </properties>
        </items>
    </content>
</jcr:root>
